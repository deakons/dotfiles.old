
! disable the scollbar
URxvt.scrollBar: 	false

URxvt.colorIT:      #00ff2b
URxvt.colorBD:      #d7d7d7
URxvt.colorUL:      #87afd7

! letter spacing for font display
URxvt.letterSpace: 	-1

! Transparency
URxvt*depth: 32
URxvt*background: rgba:0000/0000/0200/c800

! Basic setup
URxvt.font:		xft:Hack:pixelsize=16:antialias=true:style=Regular
*background: 		#232C31
*foreground: 		#C5C8C6

! Color Palette
!
! Black
*color0: #0b1c2c
*color8: #bf8b56
! Red
*color1: #223b54
*color9: #bfbf56
! Green
